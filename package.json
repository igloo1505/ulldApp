        "dev-inspect:windows": "cross-env NODE_OPTIONS='--inspect' next dev",
        "build": "next build",
        "db:generate": "dotenv -e .env.local -- prisma generate --schema=./src/database/schema.prisma",
        "db:migrate": "dotenv -e .env.local -- prisma migrate dev --name=ulldAutoMigrate",
        "db:studio": "dotenv -e .env.local -- prisma studio",
        "start": "next start",
        "lint": "tsc --watch --noEmit -p ./tsconfigLint.json",
        "runOnBuild": "tsx ./buildUtils/__TEMP__/runOnBuild.ts",
        "postinstall": "pnpm run db:generate",
        "prebuild": "pnpm run db:generate",
        "predev": "pnpm run db:generate"
    },
